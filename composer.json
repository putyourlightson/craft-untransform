{
    "name": "putyourlightson/craft-untransform",
    "description": "Replaces image transform URLs with a placeholder image or with a base URL prefix.",
    "version": "2.0.1",
    "type": "craft-plugin",
    "homepage": "https://github.com/putyourlightson/craft-untransform",
    "license": "MIT",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "transform",
        "development"
    ],
    "require": {
        "php": "^8.0.2",
        "craftcms/cms": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
          "putyourlightson\\untransform\\": "src/"
        }
    },
    "support": {
        "docs": "https://putyourlightson.com/plugins/untransform",
        "source": "https://github.com/putyourlightson/craft-untransform",
        "issues": "https://github.com/putyourlightson/craft-untransform/issues"
    },
    "extra": {
        "name": "Untransform",
        "handle": "untransform",
        "developer": "PutYourLightsOn",
        "developerUrl": "https://putyourlightson.com/",
        "changelogUrl": "https://raw.githubusercontent.com/putyourlightson/craft-untransform/v2/CHANGELOG.md",
        "class": "putyourlightson\\untransform\\Untransform"
    }
}
